{
    "version": "2",
    "templates": [
        {
            "title": "Jellyfin",
            "type": 1, 
            "categories": ["Media Server"],
            "description": "Jellyfin is a Free Software Media System that puts you in control of managing and streaming your media. It is an alternative to the proprietary Emby and Plex, to provide media from a dedicated server to end-user devices via multiple apps.",
            "logo": "https://raw.githubusercontent.com/Qballjos/portainer_templates/master/Images/jellyfin.png",
            "name": "jellyfin",
            "image": "linuxserver/jellyfin:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "ports": [
                "8096:8096/tcp",
                "7359:7359/tcp",
                "1900:1900/tcp"
              ],
            "volumes": [
                {
                  "bind": "/apps/jellyfin/config",
                  "container": "/config"
                },
                {
                    "bind": "/apps/jellyfin/cache",
                    "container": "/cache"
                },
                {
                    "bind": "/apps/jellyfin/config",
                    "container": "/media"
                },
                {
                    "bind": "/apps/jellyfin/media",
                    "container": "/config"
                },
                {
                    "bind": "/Théatre",
                    "container": "/media/Théatre"
                },
                {
                  "bind": "/Spectacle",
                  "container": "/media/Spectacle"
                },
                {
                  "bind": "/Manga",
                  "container": "/media/Manga"
                },
                {
                    "bind": "/Manga",
                    "container": "/media/Animés"
                },
                {
                    "bind": "/Manga",
                    "container": "/media/Séries"
                },
                {
                    "bind": "/Manga",
                    "container": "/media/Emission"
                },
                {
                    "bind": "/Manga",
                    "container": "/media/Films"
                },
                {
                    "bind": "/Manga",
                    "container": "/media/Tutoriels"
                }
              ]
        },
        {
            "title": "Nginx Proxy Manager",
            "type": 1,
            "categories": ["Proxy Server"],
            "description": "Nginx Proxy Manager enables you to easily forward to your websites running at home or otherwise, including free SSL, without having to know too much about Nginx or Letsencrypt.",
            "logo": "https://raw.githubusercontent.com/Qballjos/portainer_templates/master/Images/proxy_mgr.png",
            "name": "nginx-proxy-manager",
            "image": "jc21/nginx-proxy-manager:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "env": [
              {
                "label": "DB_SQLITE_FILE",
                "name": "DB_SQLITE_FILE",
                "default": "/data/database.sqlite"
              }
            ],
            "ports": [
              "80:80/tcp",
              "81:81/tcp",
              "443:443/tcp"
            ],
            
            "volumes": [
              {
                "bind": "/apps/npm/data",
                "container": "/data"
              },
              {
                "bind": "/apps/npm/data/letsencrypt",
                "container": "/etc/letsencrypt"
              }
            ]
          }
  ]
}
